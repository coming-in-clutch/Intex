@model IEnumerable<IntexProject.Models.Catalog>

@{
    ViewBag.Title = "Catalog";
}



<h2>Catalog</h2>
<div class="row">
    <div class="col-sm-8">
        <br />
        @Html.ActionLink("New Work Order", "WorkOrder", "Home", new { @class = "btn btn-primary btn-sm" })

    </div>
    <div class="col-sm-4">
        <br />
            @Html.ActionLink("See Detailed Catalog", "Catalog")

    </div>
</div>

<br />

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.assayID)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.assayName)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.TestNum)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.assayBasePrice)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.assayID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.assayName)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.TestNum)
            </td>
            <td>
                $@Html.DisplayFor(modelItem => item.assayBasePrice)
            </td>
        </tr>
    }



</table>





